<fm:ToolWindow x:Class="FM3D_Designer.src.ToolWindows.Skeleton.MeshWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FM3D_Designer.src.ToolWindows.Skeleton"
        xmlns:fm="clr-namespace:FM3D_Designer.src"
        xmlns:fb="clr-namespace:FM3D_Designer.src.ToolWindows.FileBrowser"
        mc:Ignorable="d"
        Header="Parts">
    <Grid>
        <ListBox ItemsSource="{Binding AllItems[{x:Static fb:ItemTypes.Mesh}], Source={x:Type fb:Item}}"/>
    </Grid>
</fm:ToolWindow>
